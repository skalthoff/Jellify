{
	"extends": "@react-native/typescript-config",
	"include": ["**/*.ts", "**/*.tsx"],
	"exclude": ["**/node_modules", "**/Pods"],
	"compilerOptions": {
                "paths": {
                        "@/*": ["./*"],
                        "tamagui": ["./src/material/tamagui"],
                        "tamagui/*": ["./src/material/tamagui"],
                },
		"jsx": "react-native",
		"types": ["jest", "node"],
		"skipLibCheck": true
	}
}
