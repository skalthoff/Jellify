# GitHub Repository URL
REPO_URL=https://github.com/skalthoff/jellify

# Runner registration token (get from: Settings > Actions > Runners > New self-hosted runner)
RUNNER_TOKEN=your_token_here

# Runner name prefix (each replica gets a unique suffix)
RUNNER_NAME_PREFIX=jellify-runner

# Number of parallel runners (swarm-like scaling)
RUNNER_COUNT=3
